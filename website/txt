/* Common styles for .elem elements */
.elem {
    position: relative;
    /* Add any other styles specific to .elem */
}

/* Hide the video inside each .elem by default */
.elem video {
    display: none;
}

/* Styles for the fixed-image div */
#fixed-image {
    height: 30vw;
    width: 23vw;
    border-radius: 15px;
    position: fixed;
    z-index: 99;
    left: 50%;
    top: 25%;
    display: none;
    background-size: cover;
    background-position: center;
    overflow: hidden; /* Prevent video overflow */
}

/* Make sure the video fills the entire fixed-image div */
#fixed-image video {
    width: 100%;
    height: 100%;
    object-fit: cover;
}
